        <TextBox x:Name="textBox_pay" HorizontalAlignment="Left" Height="23" Margin="200,41,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_fee" Content="Fee:" HorizontalAlignment="Left" Margin="10,66,0,0" Height="26" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_fee" HorizontalAlignment="Left" Height="23" Margin="200,69,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Version: 1.0" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top"/>
        <Label Content="€" HorizontalAlignment="Left" Margin="320,66,0,0" VerticalAlignment="Top"/>
        <Label Content="€" HorizontalAlignment="Left" Margin="320,38,0,0" VerticalAlignment="Top"/>
        <Label Content="€" HorizontalAlignment="Left" Margin="320,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
