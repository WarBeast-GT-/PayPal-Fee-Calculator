<Window x:Name="window_main" x:Class="PayPal_Fee_Calculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PayPal_Fee_Calculator"
        mc:Ignorable="d"
        Title="PayPal Fee Calculator" Height="187.815" Width="355.042">
    <Grid>
        <Label x:Name="label_vendor" Content="Price that the Vendor will receive:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
